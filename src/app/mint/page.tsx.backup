      throw new Error(
        `Pinata ì´ë¯¸ì§€ ì—…ë¡œë“œ ì‹¤íŒ¨: ${response.statusText} - ${errorText}`
      );
    }

    const result = await response.json();
    console.log("âœ… Pinata ì—…ë¡œë“œ ì„±ê³µ:", result);
    const ipfsUrl = `ipfs://${result.IpfsHash}`;
